 {\n        e.preventDefault();\n\n        var decoded = jwt_decode(token);\n        const updatedUrl = userApi + decoded._id;\n\n        const updatedUser = {\n            name: userInfo.name,\n            email: userInfo.email,\n            password: userInfo.password,\n            role: userInfo.role,\n            bio: values.bio,\n            profileImg: userInfo.profileImg\n        }\n\n        axios.put(updatedUrl, updatedUser)\n        .then(res => {\n            showBioEdit();\n        })\n        .catch(err => console.log(err));\n\n    }\n    \n        return(\n            <div className=\"profile-side\">\n                <div className=\"profile-background\">\n                <Jumbotron className=\"profile\">\n                    <div className=\"bio\">\n                            <img className=\"uploader\" src={userInfo.profileImg} alt=\"profile\"/>\n                            <div className=\"name\">\n                                <div className=\"left\">\n                                    <h3>{userInfo.name} <span className=\"type\">({userInfo.role})</span></h3>\n                                    <Dropdown>\n                                        <Dropdown.Toggle className=\"toggle\">\n                                            Notifications <Badge variant=\"light\">0</Badge>\n                                        </Dropdown.Toggle>\n                                        <Dropdown.Menu>\n                                            <Dropdown.Item eventKey=\"1\">No notifications at the moment</Dropdown.Item>\n                                        </Dropdown.Menu>\n                                    </Dropdown>\n                                </div>\n                               \n\n                                <div className=\"lorem\">\n                                    <OverlayTrigger\n                                        placement=\"right\"\n                                        delay={{ show: 250, hide: 400 }}\n                                        overlay={renderTooltip}\n                                    >\n                                    <Button className=\"bio-button\" onClick={()=> showBioEdit()} hidden={!editShow}>{userInfo.bio}</Button>\n                                    </OverlayTrigger>\n                                    <Form onSubmit={updateUser}>\n                                    <Form.Group controlId=\"bio\" hidden={editShow}>\n                                        <Form.Label>Edit Bio</Form.Label>\n                                        <Form.Control as=\"textarea\" type=\"text\" rows={1} name=\"bio\" value={values.bio} onChange={handleChange}/>\n                                        <Button className=\"green\" onClick={()=> showBioEdit()}>Cancel</Button><Button type=\"submit\" className=\"green\">Submit</Button>\n                                    </Form.Group>\n                                    </Form>\n                                </div> \n                            </div>\n                    </div>\n                </Jumbotron>\n\n                <Jumbotron className=\"about\">\n\n                    <div className=\"center product-scroll products\">\n                        {userInfo.role === \"investor\" ? (\n                        <h1 className=\"type new-products\">My Investments</h1>\n                        ): <h1 className=\"type new-products\">My Products</h1>}\n                    \n            \n                        {userInfo.products < 1 ? (\n                            <h1 className=\"white\">Nothing to see here... Hit the button below to add a product!</h1>\n                        ): <CurrentUserProducts/>}\n\n                    </div>\n    \n                    <div className=\"center lorem\">\n                        {userInfo.role === \"investor\" ? (\n                        <Button className=\"add\"><Link className=\"link\" to='/products'>Find products</Link></Button>\n                        ): <Button className=\"add\"><Link className=\"link\" to='/addProduct'>Create new product</Link></Button>}\n                    </div>\n\n                </Jumbotron>\n            </div>\n            </div>\n        )\n}\n    \n\nexport default Profile; \n\n","/home/ed/reactprojects/invest/src/Components/LogIn/logIn.jsx",[],"/home/ed/reactprojects/invest/src/Components/BusinessPlanBuilder/businessPlanBuilder.jsx",[],"/home/ed/reactprojects/invest/src/Components/BusinessPlanBuilder/Steps/Introduction/introduction.jsx",[],"/home/ed/reactprojects/invest/src/Components/BusinessPlanBuilder/Steps/stepOne/stepOne.jsx",[],"/home/ed/reactprojects/invest/src/Components/BusinessPlanBuilder/Steps/stepThree/stepThree.jsx",[],"/home/ed/reactprojects/invest/src/Components/BusinessPlanBuilder/Steps/stepFour/stepFour.jsx",[],"/home/ed/reactprojects/invest/src/Components/BusinessPlanBuilder/Steps/stepTwo/stepTwo.jsx",[],"/home/ed/reactprojects/invest/src/Components/BusinessPlanBuilder/Steps/stepFive/stepFive.jsx",[],"/home/ed/reactprojects/invest/src/Components/Error404/error404.jsx",[],"/home/ed/reactprojects/invest/src/Components/About/about.jsx",[],"/home/ed/reactprojects/invest/src/Components/Footer/footer.jsx",[],"/home/ed/reactprojects/invest/src/Components/UseForm/useForm.jsx",[],"/home/ed/reactprojects/invest/src/utils/setAuthToken.js",[],"/home/ed/reactprojects/invest/src/Components/Auth/auth.js",[],"/home/ed/reactprojects/invest/src/Components/NavBar/navBar.jsx",[],"/home/ed/reactprojects/invest/src/Components/Investors/investors.js",[],"/home/ed/reactprojects/invest/src/Components/Products/products.js",[],"/home/ed/reactprojects/invest/src/Components/Products/SearchBar/searchBar.js",[],"/home/ed/reactprojects/invest/src/Components/Products/ProductList/productList.js",[],"/home/ed/reactprojects/invest/src/Components/Investors/InvestorList/investorList.js",[],"/home/ed/reactprojects/invest/src/Components/Investors/SearchBar/searchBar.js",[],"/home/ed/reactprojects/invest/src/Components/Profile/C